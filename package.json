{
  "name": "discord-akairo-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate with useful starting commands and MongoDB integration",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon ./src/Bot.ts",
    "dev:debug": "nodemon --inspect ./src/Bot.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^14.14.7",
    "nodemon": "^2.0.6",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "@types/mongoose": "^5.10.0",
    "discord-akairo": "^8.1.0",
    "discord.js": "^12.4.1",
    "dotenv": "^8.2.0",
    "mongodb": "^3.6.3",
    "mongoose": "^5.10.14",
    "tslog": "^2.11.1",
    "typeorm": "^0.2.29"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "ext": "ts",
    "env": {
      "NODE_ENV": "development"
    },
    "execMap": {
      "ts": "node --require ts-node/register"
    }
  }
}
